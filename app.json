{
  "expo": {
    "name": "Marketplace Eventi e Fiere",
    "slug": "marketplace-eventi-e-fiere",
    "scheme": "marketplace",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventiefiere.marketplace"
    },
    "android": {
      "package": "com.eventiefiere.marketplace",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}